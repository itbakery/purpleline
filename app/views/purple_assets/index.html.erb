<h1>Listing purple_assets</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Type</th>
    <th>Attachable</th>
    <th>Attachable type</th>
    <th>Publish</th>
    <th>Latitude</th>
    <th>Longtitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @purple_assets.each do |purple_asset| %>
  <tr>
    <td><%= purple_asset.title %></td>
    <td><%= purple_asset.type %></td>
    <td><%= purple_asset.attachable_id %></td>
    <td><%= purple_asset.attachable_type %></td>
    <td><%= purple_asset.publish %></td>
    <td><%= purple_asset.latitude %></td>
    <td><%= purple_asset.longtitude %></td>
    <td><%= link_to 'Show', purple_asset %></td>
    <td><%= link_to 'Edit', edit_purple_asset_path(purple_asset) %></td>
    <td><%= link_to 'Destroy', purple_asset, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

