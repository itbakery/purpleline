<h1>Listing pages_translations</h1>

<table>
  <tr>
    <th>Page</th>
    <th>Language</th>
    <th>Title</th>
    <th>Content</th>
    <th>Publish</th>
    <th>Start on</th>
    <th>Stop on</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @pages_translations.each do |pages_translation| %>
  <tr>
    <td><%= pages_translation.page_id %></td>
    <td><%= pages_translation.language_id %></td>
    <td><%= pages_translation.title %></td>
    <td><%= pages_translation.content %></td>
    <td><%= pages_translation.publish %></td>
    <td><%= pages_translation.start_on %></td>
    <td><%= pages_translation.stop_on %></td>
    <td><%= pages_translation.user_id %></td>
    <td><%= link_to 'Show', pages_translation %></td>
    <td><%= link_to 'Edit', edit_pages_translation_path(pages_translation) %></td>
    <td><%= link_to 'Destroy', pages_translation, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

