<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", admin_progresses_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_progress_path %></li>
    </ul>
  </div>          
  <div class="content">          
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> Progresses</h2>
    <div class="inner">
      <table class="table">
        <tr>             
          <th class="first">ID</th>
           <th>
            <%= t("activerecord.attributes.admin_progress.issuedate", :default => t("activerecord.labels.issuedate", :default => "IssueDate")) %>
          </th>          
           <th>
            <%= t("activerecord.attributes.admin_progress.month", :default => t("activerecord.labels.month", :default => "Month")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @progresses.each do |progress| -%>
        <tr class="<%= cycle("odd", "even") %>">

          <td>
            <%= progress.id %>
          </td>
          <td>
            <%= progress.issuedate %>
          </td>
          <td>
            <%= link_to progress.month, admin_progress_path(progress) %>
          </td>
          <td>
            <%= display_date(progress.created_at) %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", admin_progress_path(progress) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_progress_path(progress) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", admin_progress_path(progress), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>          
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
