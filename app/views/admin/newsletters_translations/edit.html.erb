<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", admin_newsletters_translations_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_admin_newsletters_translation_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_admin_newsletters_translation_path %></li>
    </ul>
  </div>          
  <div class="content">            
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> NewslettersTranslation</h2>
    <div class="inner">
      <%= form_for @newsletters_translation, :url => admin_newsletters_translation_path(@newsletters_translation), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>